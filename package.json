{
  "name": "@adopted-ember-addons/ember-stripe-elements",
  "version": "1.0.0-rc.2",
  "description": "A simple Ember wrapper for Stripe Elements",
  "keywords": [
    "credit card",
    "ember-addon",
    "stripe",
    "stripe element",
    "stripe elements"
  ],
  "repository": "https://github.com/adopted-ember-addons/ember-stripe-elements",
  "license": "MIT",
  "author": "Josh Smith",
  "directories": {
