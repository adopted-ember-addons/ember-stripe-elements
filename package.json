    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.15.0",
    "eslint-plugin-ember": "^10.0.2",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "travis-deploy-once": "^5.0.0"
